        <div style="display: flex; justify-content: right; list-style:none">
          
            <button type="submit" style="margin-right: 20px;" class="btn btn-warning"><a style="color: black; text-decoration: none;" href="/register">Login</a></button>
        
            <button type="button" style="margin-right: 20px; " class="btn btn-outline-warning"><a style="color: black; text-decoration: none;" href="tel:+9667598287">Call Now</a></button>
        </div>

    </div>
</div>
   
    <hr/>
    <div style="margin-top: 10px;" class="container">
        <main>
          <div class="container" style="margin-left: 350px;">
            <button style="width: 500px; height: 100px; font-size: x-large;" class="btn btn-warning rounded-pill " type="button">Book an appointment</button>
            <br/>
            <form>
      
                  <div class="col-md-5">
                    <label for="country" class="form-label">Country</label>
                    <select class="form-select" id="country" required="">
                      <option value="">Choose...</option>
                      <option>India</option>
                      
                    </select>
                    <div class="invalid-feedback">
                      Please select a valid country.
                    </div>
                  </div>
      
                  <div class="col-md-5">
                    <label for="state" class="form-label">State</label>
                    <select class="form-select" id="state" required="">
                      <option value="">Choose...</option>
                      <option>Mumbai</option>
                      <option>Delhi</option>
                    </select>
                    <div class="invalid-feedback">
                      Please provide a valid state.
                    </div>
                  </div>
                  
                  <div class="col-md-5">
                    <label for="service" class="form-label">Service</label>
                    <select class="form-select" id="service" required="">
                      <option value="">Choose...</option>
                      <option>Consultation</option>
                      <option>Dentistry</option>
                      <option>Eyecare</option>
                      <option>Vaccination</option>
                      <option>Pathology</option>
                      <option>Pet Boarding</option>
                      
                    </select>
                    <div class="invalid-feedback">
                      Please select a valid service.
                    </div>
                  </div>

                  <div class="col-md-5">
                    <label for="day" class="form-label">Date</label>
                    <br/>
                    <input type="date" id="date" name="date" required="">
                      
                    </select>
                    <div class="invalid-feedback">
                      Please select a valid date.
                    </div>
                  </div>

                  <div class="col-md-5">
                    <label for="service" class="form-label">Time</label>
                    <select class="form-select" id="service" required="">
                      <option value="">Choose...</option>
                      <option>10:00 AM</option>
                      <option>11:00 AM</option>
                      <option>12:00 PM</option>
                      <option>01:00 PM</option>
                      <option>02:00 PM</option>
                      <option>04:00 PM</option>
                      <option>05:00 PM</option>
                      <option>06:00 PM</option>
                      <option>07:00 PM</option>
                      <option>08:00 PM</option>
                      <option>09:00 PM</option>
                      
                    </select>
                    <div class="invalid-feedback">
                      Please select a valid date.
                    </div>
                  </div>
                  <br/>
      
                <button class="w-20 btn btn-success " type="submit" on >Continue to checkout</button>
            </form>
           
          </div>
          
        </main>
      
        
      </div>
