     <div style="height: 300px; width: 700px; margin-top: 100px; margin-bottom: 50px; " class="container">
        <main class="form-signin w-100 m-auto">
            <form action="/register" method="post">
              
              
              
              <div style="border: 1px  solid black; padding: 20px;">
                <h1 class="h3 mb-3 fw-normal">Please Register</h1>
              <div class="form-floating">
                <input style="margin-bottom: 2px;" type="name" name="name" class="form-control" id="floatingInput" placeholder="name" required="true">
                <label for="floatingInput">Name</label>
              </div>
                <div class="form-floating">
                  <input  style="margin-bottom: 2px;" type="email" name="email" class="form-control" id="floatingInput" placeholder="name@example.com" required="true">
                  <label for="floatingInput">Email address</label>
                </div>
                
                <div class="form-floating">
                  <input  style="margin-bottom: 2px;" type="password" name="password" class="form-control" id="floatingPassword" placeholder="Password" required="true">
                  <label for="floatingPassword">Password</label>
                </div>
                
              
                <button  style="margin-bottom: 2px;" class="btn btn-warning w-100 py-2" type="submit">Register</button>
                <button  style="margin-bottom: 2px;" class="btn btn-light w-100 py-2" type="submit"><a style="color: black; text-decoration: none;" href="/login">Click here to Login</a></button>
              </div>
              
              
            </form>
